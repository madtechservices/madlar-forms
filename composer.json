{
    "name": "tomatophp/tomato-forms",
    "type": "library",
    "description": "Build a full Forms with Fields on Database and save it to user meta",
    "keywords": [
        "php",
        "laravel",
        "forms",
        "form builder",
        "database-forms",
        "form-component",
        "splade",
        "tomatophp"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\TomatoForms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\TomatoForms\\TomatoFormsServiceProvider"
            ],
            "aliases": {
                "TomatoForms": "TomatoPHP\\TomatoForms\\Facades\\TomatoForms"
            }
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "tomatophp/console-helpers": "^1.0",
        "tomatophp/tomato-admin": "^1.0",
        "spatie/laravel-translatable": "*",
        "tomatophp/tomato-translations": "^1.1"
    }
}
